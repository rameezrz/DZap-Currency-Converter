{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "jsx": "react",
    "outDir": "./dist",
    "baseUrl": "./"
  },
  "include": ["src/**/*.ts","docs/swagger.ts"],
  "exclude": ["node_modules"]
}
